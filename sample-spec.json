          type: "string"
      rates:
        type: "array"
        description: "ordered list of rate.id's to display"
        items:
          type: "string"
      priceSchedules:
        type: "array"
        description: "Prices"
        items:
          $ref: "#/definitions/PriceSchedule"
  PriceSchedule:
    type: object
