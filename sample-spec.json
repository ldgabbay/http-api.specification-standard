      parameters:
        - name: "code"
          in: "query"
          description: "The currency code"
          required: true
          type: "string"
      responses:
        200:
          description: "OK"
          schema:
            $ref: "#/definitions/CurrencyResponse"
        404:
          description: "Not Found"

    ],
